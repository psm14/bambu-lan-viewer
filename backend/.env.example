PRINTER_HOST=192.168.1.123
PRINTER_SERIAL=XXXXXXXXXXXXXXX
PRINTER_ACCESS_CODE=YOUR_LAN_ACCESS_CODE

# MQTT settings
MQTT_TLS=1
MQTT_TLS_INSECURE=1
MQTT_PORT=8883
# MQTT_CA_CERT=/path/to/printer-ca.pem
MQTT_CLIENT_ID=bambu-lan-viewer
MQTT_KEEP_ALIVE_SECS=30
MQTT_USER_ID=1
MQTT_MAX_INCOMING_PACKET_SIZE=262144
MQTT_MAX_OUTGOING_PACKET_SIZE=65536

# RTSP settings
# RTSP_URL=rtsps://192.168.1.123:322/streaming/live/1
RTSP_PORT=322
RTSP_PATH=/streaming/live/1
RTSP_USERNAME=bblp
# RTSP_PASSWORD=YOUR_LAN_ACCESS_CODE
RTSP_TLS_INSECURE=1

# HLS output
HLS_OUTPUT_DIR=hls
HLS_TARGET_DURATION_SECS=2
HLS_WINDOW_SEGMENTS=6

# HTTP server bind address
HTTP_BIND=0.0.0.0:8080
